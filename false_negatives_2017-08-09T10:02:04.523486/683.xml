  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A High Statistics Study of Upsilon Meson Production in $\pi^- W$ Reactions at 286-{GeV}/$c$</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">A HIGH STATISTICS STUDY OF upsilon MESON PRODUCTION IN pi- W REACTIONS AT 286-GeV/c</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1986</subfield>
    <subfield code="t">published</subfield>
  </datafield>
