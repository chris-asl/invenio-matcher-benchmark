  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Theoretical investigation of the process gamma gamma ---&gt; pi pi at low energies</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1971</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
