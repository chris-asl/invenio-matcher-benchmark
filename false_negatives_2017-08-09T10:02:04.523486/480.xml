  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Ternary fission produced in Au, Bi, Th and U with Ar ions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1969</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
