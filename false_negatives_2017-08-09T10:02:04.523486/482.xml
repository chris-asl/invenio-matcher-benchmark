  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Polarization in 11 B(p, p) and 11 B(p, p′) at 30.3 MeV</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1969</subfield>
