  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The strongly damped collisions of 120 MeV 20 Ne with 27 Al</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1977-02-21</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
