  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The (t, p) reaction with the even isotopes of Ca</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1967</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
