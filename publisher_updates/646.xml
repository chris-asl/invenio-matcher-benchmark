  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Magnetic moment for the negative parity Λ → Σ 0 transition in light cone QCD sum rules</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07-10</subfield>
