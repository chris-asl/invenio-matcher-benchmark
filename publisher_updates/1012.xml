  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Exotic resonances due to η exchange</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-10</subfield>
