  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Application of AdS/QCD to B physics</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-04</subfield>
