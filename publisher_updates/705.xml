  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Characterization of the ETEL D784UKFLB 11 in. photomultiplier tube</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04-21</subfield>
