  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Gauge U (1) dark symmetry and radiative light fermion masses</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-10</subfield>
    <subfield code="t">published</subfield>
