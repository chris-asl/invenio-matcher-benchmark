  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Neutrino mixing: A 4 variations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-10</subfield>
