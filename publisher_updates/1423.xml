  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Aligned natural inflation with modulations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08-10</subfield>
