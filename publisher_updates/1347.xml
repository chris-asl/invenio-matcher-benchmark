  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Affleck–Dine leptogenesis and its backreaction to inflaton dynamics</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-10</subfield>
    <subfield code="t">published</subfield>
