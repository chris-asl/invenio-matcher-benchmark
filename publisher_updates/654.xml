  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Classical interactions of the instanton-dyons with antidyons</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06</subfield>
    <subfield code="t">published</subfield>
