  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Baseline measures for net-proton distributions in high energy heavy-ion collisions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03</subfield>
