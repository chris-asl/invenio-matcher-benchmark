  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Dissipative universe-inflation with soft singularity</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-03</subfield>
