  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Medium-induced jet evolution: multiple branching and thermalization</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-01</subfield>
