 <datafield tag="245" ind1=" " ind2=" ">
  <subfield code="a">Physical quantities and dimensional analysis: from mechanics to quantum gravity</subfield>
  <subfield code="9">Sociedade Brasileira de Física</subfield>
 </datafield>
 <datafield tag="041" ind1=" " ind2=" ">
  <subfield code="a">Portuguese</subfield>
