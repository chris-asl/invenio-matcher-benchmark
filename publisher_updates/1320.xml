  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Flavour symmetries in a renormalizable SO (10) model</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-05</subfield>
