  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Full analysis of general non-standard tbW couplings</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-10</subfield>
