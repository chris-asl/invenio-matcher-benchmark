  <subfield code="m">\begin{equation*} \begin{align*} \begin{eqnarray}&amp;&amp;\begin{array}{*{35}{l}} {{S}^{\alpha}}{{S}_{\alpha}}\geqslant 0. \end{array}\end{eqnarray} \end{align*}</subfield>
  <subfield code="9">refextract</subfield>
