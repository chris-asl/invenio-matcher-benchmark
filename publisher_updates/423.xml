  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A new technique for infrared scintillation measurements</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-05-21</subfield>
    <subfield code="t">published</subfield>
