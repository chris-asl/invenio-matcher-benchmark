  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Quark CP -phase and Froggatt–Nielsen mechanism</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08-10</subfield>
