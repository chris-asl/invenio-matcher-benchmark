  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Continuum limit of the leading-order HQET form factor in B s → K ℓν decays</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06-10</subfield>
