  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The odd-order Pais–Uhlenbeck oscillator</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06</subfield>
