  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">New family of Maxwell like algebras</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08-10</subfield>
    <subfield code="t">published</subfield>
