  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">F-theory vacua with <math altimg="si1.gif" display="inline" overflow="scroll"><msub><mrow><mi mathvariant="double-struck">Z</mi></mrow><mrow><mn>3</mn></mrow></msub></math> gauge symmetry</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-09</subfield>
