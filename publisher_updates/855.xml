  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Nuclear medium effects in structure functions of nucleon at moderate <math altimg="si1.gif" display="inline" overflow="scroll"><msup><mrow><mi>Q</mi></mrow><mrow><mn>2</mn></mrow></msup></math></subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
