  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Killing tensors in stationary and axially symmetric space–times</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-05</subfield>
