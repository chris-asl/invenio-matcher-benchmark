  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Scalar and vector self-energies of heavy baryons in nuclear medium</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04</subfield>
