  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Surface delta interaction in the g 7/2 − d 5/2 model space</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-05</subfield>
