  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">N=1 Supersymmetric Non-Abelian Compensator Mechanism for Extra Vector Multiplet</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">N=1 Supersymmetric Non-Abelian Compensator Mechanism for Extra Vector Multiplet</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
