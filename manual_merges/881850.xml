  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Enabling comparison of UrQMD with Geant4 hadronic models</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Enabling comparison of UrQMD with Geant4 hadronic models</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2010-12</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
