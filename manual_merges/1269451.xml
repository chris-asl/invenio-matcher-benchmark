  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A new method to determine large scale structure from the luminosity distance</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">A new inversion method to map LTB solutions to luminosity distance</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">A new method to determine large scale structure from the luminosity distance</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
