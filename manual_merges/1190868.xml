  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Priomordial non-Gaussian features from DBI Galileon inflation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2012-10</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
