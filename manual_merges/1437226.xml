  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Optical-model analysis for 6 Li elastic scattering by 16 O at 20 MeV</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1970</subfield>
