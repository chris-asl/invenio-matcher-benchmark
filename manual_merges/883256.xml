  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The Kinetics Of Nonequilibrium Universe. I. The Condition Of Local Thermodynamical Equilibrium</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">The Kinetics Of Nonequilibrium Universe. I. The Condition Of Local Thermodynamical Equilibrium</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
