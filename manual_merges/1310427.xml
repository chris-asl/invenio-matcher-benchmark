  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Infinitely many inequivalent field theories from one Lagrangian</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Infinitely many inequivalent field theories from one Lagrangian</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-08-11</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
