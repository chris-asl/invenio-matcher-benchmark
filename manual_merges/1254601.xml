  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Nuclear and neutron matter $G$-matrix calculations with Ch-EFT potential including effects of three-nucleon interaction</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Nuclear and neutron matter $G$-matrix calculations with Ch-EFT potential including effects of three-nucleon interaction</subfield>
    <subfield code="9">arXiv</subfield>
