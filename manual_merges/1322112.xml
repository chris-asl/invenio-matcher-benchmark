  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Dimension-six anomalous $tq\gamma$ couplings in $\gamma \gamma$ collision at the LHC</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Dimension-six anomalous $tq\gamma$ couplings in $\gamma \gamma$ collision at the LHC</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-10-14</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
