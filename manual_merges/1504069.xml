  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Next-to-leading order QCD corrections to $\chi_{cJ} W^+ b$ associated production from top-quark decay</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Next-to-leading order QCD corrections to $\chi_{cJ} W^+ b$ associated production from top-quark decay</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
