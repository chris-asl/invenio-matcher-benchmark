  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Thermodynamical instabilities of perfect fluid spheres in General Relativity</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Thermodynamical instabilities of perfect fluid spheres in General Relativity</subfield>
  </datafield>
