  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Three-body treatment of the penetration through the Coulomb field of a two-fragment nucleus</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Three-body treatment of the penetration through the Coulomb field of a two-fragment nucleus</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
