  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">J/{\Psi} and {\Psi}(2S) Production in p-Pb Collisions at 5.02 TeV with ATLAS</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">J/{\Psi} and {\Psi}(2S) Production in p-Pb Collisions at 5.02 TeV with ATLAS</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-09-22</subfield>
  </datafield>
