  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Duality-symmetric Action for Electromagnetic Theory with Electric and Magnetic Sources</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Duality-symmetric Action for Electromagnetic Theory with Electric and Magnetic Sources</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-06-24</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
