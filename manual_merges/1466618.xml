  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">iResum: a new paradigm for resumming gravitational wave amplitudes</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">iResum: a new paradigm for resumming gravitational wave amplitudes</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2016-06-01</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
