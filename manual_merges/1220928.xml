  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Direct Photon Measurement in Proton-Proton and Deuteron-Gold Collisions</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
