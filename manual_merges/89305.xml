  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Absorption corrections in a mueller regge analysis of inclusive photo- and electroproduction</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1974</subfield>
