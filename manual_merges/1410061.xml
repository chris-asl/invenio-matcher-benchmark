  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Stability of a noncanonical scalar field model during cosmological date</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
