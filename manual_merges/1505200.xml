  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Oscillation characteristics of active and sterile neutrinos and neutrino anomalies at shot distances</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Oscillation characteristics of active and sterile neutrinos and neutrino anomalies at shot distances</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
