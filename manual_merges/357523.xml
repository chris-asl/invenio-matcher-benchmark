  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A Search for exact superstring vacua</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1994</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
