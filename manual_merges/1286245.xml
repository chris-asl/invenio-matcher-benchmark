  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Are non-Fermi-liquids stable to Cooper pairing?</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-03-14</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
