  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">An introduction to motivic zeta functions of motives</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2008-12-19</subfield>
