  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Spatially Resolving the Very High Energy emission from MGRO J2019+37 with VERITAS</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Spatially Resolving the Very High Energy emission from MGRO J2019+37 with VERITAS</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-04-07</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
