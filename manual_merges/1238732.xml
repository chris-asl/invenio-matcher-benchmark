  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Overview of beam-beam effects in the Tevatron</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-04-18</subfield>
