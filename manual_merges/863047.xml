  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Colliding Hadrons as Cosmic Membranes and Possible Signatures of Lost Momentum</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Colliding Hadrons as Cosmic Membranes and Possible Signatures of Lost Momentum</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2010-07</subfield>
  </datafield>
