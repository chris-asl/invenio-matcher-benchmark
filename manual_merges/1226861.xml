  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Results on antiproton-proton elastic scattering</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1959-06-16</subfield>
