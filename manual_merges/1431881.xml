  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On the description of fermion systems in boson representations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1967</subfield>
    <subfield code="t">published</subfield>
