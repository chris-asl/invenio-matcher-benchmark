  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">First-forbidden transitions and stellar $\beta$-decay rates of Zn and Ge isotopes</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">First-forbidden transitions and stellar $\beta$-decay rates of Zn and Ge isotopes</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
