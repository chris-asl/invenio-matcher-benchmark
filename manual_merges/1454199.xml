  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Excitation Entanglement Entropy in 2d Conformal Field Theories</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
