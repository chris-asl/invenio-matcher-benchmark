  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">LHC signals of the SO(5)$\times$U(1) gauge-Higgs unification</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
