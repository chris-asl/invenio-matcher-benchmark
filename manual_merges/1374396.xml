  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Baryon femtoscopy considering residual correlations as a tool to extract strong interaction potentials</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
    <subfield code="t">published</subfield>
