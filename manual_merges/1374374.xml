  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">MuSun: muon capture on the deuteron</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
