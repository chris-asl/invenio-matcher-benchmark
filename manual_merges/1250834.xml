  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A measurement of the energy and timing resolution of the GlueX Forward Calorimeter using an electron beam</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2013</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
