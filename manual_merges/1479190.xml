  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Probing the resonance of Dirac particle by the application of complex momentum representation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Probing the resonance of Dirac particle by the application of complex momentum representation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2016-08-02</subfield>
  </datafield>
