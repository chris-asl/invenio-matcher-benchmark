  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Precision calculations for the $T$-odd quark pair production at the CLIC $e^+e^-$ linear collider</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Precision calculations for the $T$-odd quark pair production at the CLIC $e^+e^-$ linear collider</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-06-19</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
