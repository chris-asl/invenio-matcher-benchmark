  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Symmetries of the Schr\"odinger Equation and Algebra/Superalgebra Duality</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Symmetries of the Schr\"odinger Equation and Algebra/Superalgebra Duality</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-11-28</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
