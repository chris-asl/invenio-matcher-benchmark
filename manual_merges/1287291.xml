  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Investigation of Bouncing Universe and Phantom Crossing in Modified Gravity Coupled with Weyl Tensor and its Reconstruction</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-03-24</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
