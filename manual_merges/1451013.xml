  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A simple approach to nuclear transition regions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1985</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
