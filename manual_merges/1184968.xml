  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Resolving Octant Degeneracy at FNAL-LBNE using Daya Bay Reactor Experiment</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Resolving Octant Degeneracy at FNAL-LBNE using Daya Bay Reactor Experiment</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Resolving Octant Degeneracy at LBL experiment by combining Daya Bay Reactor Setup</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2012-09</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
