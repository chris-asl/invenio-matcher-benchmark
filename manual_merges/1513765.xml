  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Constraints on the location of the gamma-ray emission region for the gamma-ray-loud radio source GB 1310+487</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Constraints on the location of the gamma-ray emission region for the gamma-ray-loud radio source GB 1310+487</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
