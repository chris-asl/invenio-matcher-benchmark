  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Hard exclusive reactions and generalized parton distributions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
