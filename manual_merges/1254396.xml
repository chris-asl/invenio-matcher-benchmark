  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Energy spectra of KASCADE-Grande based on shower size measurements and different hadronic interaction models</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Energy spectra of KASCADE-Grande based on shower size measurements and different hadronic interaction models</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-09-17</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
