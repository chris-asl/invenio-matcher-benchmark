  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Gravitational collapse of a magnetized fermion gas with finite temperature</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
