  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Emptiness of the Universe as a Cause of Inflation and Dark Energy</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
