  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Resolution and Efficiency of the ATLAS Muon Drift-Tube Chambers at High Background Rates</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2016-03-31</subfield>
