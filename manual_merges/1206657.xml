  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Casimir Energy for a Fermion Coupled to the Sine-Gordon Model</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Casimir Energy for a Fermion Coupled to the Sine-Gordon Model</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">An Investigation of the Casimir Energy for a Fermion Coupled to the Sine-Gordon Soliton with Parity Decomposition</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2012-12</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
