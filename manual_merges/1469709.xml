  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Planning and Controlling of the Cold Accelerator Sections Installation in XFEL</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
