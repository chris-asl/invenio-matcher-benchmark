  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Operating experience with electron cloud clearing electrodes at DAFNE</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
