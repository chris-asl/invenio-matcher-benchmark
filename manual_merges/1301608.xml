  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Energy of the low-lying bound S-states of exotic two-muon three-body systems</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-06-19</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
