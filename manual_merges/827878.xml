    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Interacting new agegraphic dark energy in non-flat Brans-Dicke cosmology</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2009-08</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
