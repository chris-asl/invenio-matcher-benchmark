  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Study of Magnetic Hysteresis Effects in a Storage Ring Using Precision Tune Measurement</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
