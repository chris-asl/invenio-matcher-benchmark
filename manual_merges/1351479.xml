  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">The role of field redefinition on renormalisability of a general $N=\frac{1}{2}$ supersymmetric gauge theories</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">The role of field redefinition on renormalisability of a general $N=\frac{1}{2}$ supersymmetric gauge theories</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-03-10</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
