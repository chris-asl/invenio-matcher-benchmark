  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Positive pion production by 185 mev protons</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1974</subfield>
