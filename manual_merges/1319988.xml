  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Dynamics of anisotropic collapsing spheres in Einstein Gauss–Bonnet gravity</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Dynamical Stability of Collapsing Stars in Einstein Gauss-Bonnet Gravity</subfield>
  </datafield>
