  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Discovery of the compact X-ray source inside the Cygnus Loop</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">1998-07</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
