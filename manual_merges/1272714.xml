  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Neutrinos in the Early Universe, Kalb-Ramond Torsion and Matter-Antimatter Asymmetry</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Neutrinos in the Early Universe, Kalb-Ramond Torsion and Matter-Antimatter Asymmetry</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
