  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Renormalizable Noncommutative U(1) Gauge Theory Without IR/UV Mixing</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Renormalizable Noncommutative U(1) Gauge Theory Without IR/UV Mixing</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">On the Renormalizability of Noncommutative U(1) Gauge Theory - an Algebraic Approach</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2009-02</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
