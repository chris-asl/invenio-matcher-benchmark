  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Experimental study of the reaction $\pi^- p \to \Lambda K^0$ at beam momenta between 930 and 1130 MeV/c</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">1975-01</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
