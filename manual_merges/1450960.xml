  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Relativistic coulomb collisions and the virtual radiation spectrum</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1985</subfield>
