  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">PHENOMENOLOGICAL MINIMAL LOW-ENERGY SUPERGRAVITY WITHOUT HIGGS MIXING</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1986</subfield>
