  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Application of the RMF mass model to the $r$-process and the influence of mass uncertainties</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Application of the RMF mass model to the r-process and the influence of mass uncertainties</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
