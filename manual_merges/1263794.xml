  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Nuclear deformation and neutron excess as competing effects for pygmy dipole strength</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Nuclear deformation and neutron excess as competing effects for pygmy dipole strength</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-11-08</subfield>
  </datafield>
