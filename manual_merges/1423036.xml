  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">$\beta$-decay half-lives and nuclear structure of exotic proton-rich waiting point nuclei under $rp$-process conditions</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">$\beta$-decay half-lives and nuclear structure of exotic proton-rich waiting point nuclei under $rp$-process conditions</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2016-02-23</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
