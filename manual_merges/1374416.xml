  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Performance of the muon identification and reconstruction with the ATLAS detector</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
