  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Measurement of the mixing angle in pseudoscalar meson sector in $\pi^{-}$ and $K^{-}$ beams with GAMS-$4\pi$ Setup</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Measurement of the $\eta$ -$\eta'$ mixing angle in $\pi^{-}$ and $K^{-}$ beams with GAMS-$4\pi$ Setup</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-01</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
