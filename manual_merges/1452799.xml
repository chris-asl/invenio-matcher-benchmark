  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Revisiting van der Waals like behavior of f(R) AdS black holes via the two point correlation function</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Van der Waals like behavior and equal area law of two point correlation function of f(R) AdS black holes</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Revisiting van der Waals like behavior of f(R) AdS black holes via the two point correlation function</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
