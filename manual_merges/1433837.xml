  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">An experimental study on time-reversal invariance in nuclear gamma decay</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1968</subfield>
    <subfield code="t">published</subfield>
