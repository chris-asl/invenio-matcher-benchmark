  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">One loop corrections to the lightest Higgs mass in the minimal eta model</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">1993-03</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
