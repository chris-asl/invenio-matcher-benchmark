  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Detection of supernova neutrinos on the Earth for large theta13</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Detection of supernova neutrinos on the Earth for large theta13</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-03-04</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
