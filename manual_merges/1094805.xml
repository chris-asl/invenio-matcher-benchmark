  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Anisotropic Bianchi-I Cosmological Model in String Cosmology with Variable Deceleration Parameter</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Anisotropic Bianchi-I Cosmological Models in String Cosmology with Variable Deceleration Parameter</subfield>
  </datafield>
