  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A Search for Direct CP Violation in $K^{\pm} \to \pi^{\pm} \pi^{\pm} \pi^{\mp}$ Decays</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">A search for direct CP violation in K+- --> pi+- pi+- pi-+ decays.</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2000</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
