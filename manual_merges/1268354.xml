  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Interface Tension and Interface Entropy in 2+1 Flavour Nambu-Jona-Lasino Model</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Interface Tension and Interface Entropy in 2+1 Flavour Nambu-Jona-Lasino Model</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-12-08</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
