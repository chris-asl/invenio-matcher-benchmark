  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Chirally extended QCD or an asymptotically free chiral linear sigma model with quarks coupled to gluons</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Chirally Extended QCD or An Asymptotically Free Chiral Linear Sigma Model With Quarks Coupled to Gluons</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
