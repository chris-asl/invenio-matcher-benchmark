  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">New formulae for the $(-2)$ moment of the photo-absorption cross section, $\sigma_{_{-2}}$</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-10-14</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
