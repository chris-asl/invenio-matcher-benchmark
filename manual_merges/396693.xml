  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Interaction hierarchy: Gonihedric string and quantum gravity</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">1995-06</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
