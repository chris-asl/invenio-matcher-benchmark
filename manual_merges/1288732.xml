  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Quasi-particle and collective magnetism: Rotation, pairing and blocking in high-K isomers</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-04-03</subfield>
  </datafield>
