  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Equation of state of strongly interacting matter in compact stars</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2006-03-21</subfield>
