  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Lorentz invariant 'elements of reality' and the question of joint measurability of commuting observables</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">1993-05</subfield>
