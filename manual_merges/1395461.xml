  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Study of the $f_2(1270)$ and $a_2(1320)$ resonances in $\gamma^*(Q^2)\gamma$ collisions</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
