  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Low-energy kaon-nucleon/nuclei interaction studies at DAΦNE by AMADEUS</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
