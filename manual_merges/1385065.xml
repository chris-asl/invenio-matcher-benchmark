  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The lightcurve of the macronova associated with the long-short burst GRB 060614</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">The lightcurve of the macronova associated with the long-short burst GRB 060614</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
