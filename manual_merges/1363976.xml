  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Analyzing Surface Roughness Dependence of Linear RF Losses</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2013-04</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
