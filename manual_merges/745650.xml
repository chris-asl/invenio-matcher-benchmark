  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Quark mass ratios in ChPT with the difference of quark condensates considered.</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
