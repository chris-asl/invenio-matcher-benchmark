  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Dynamical Analysis of Charged Anisotropic Spherical Star in f(R) Gravity</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-10-11</subfield>
  </datafield>
  <datafield tag="500" ind1=" " ind2=" ">
