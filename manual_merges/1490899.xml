  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Fits of Weak Annihilation and Hard Spectator Scattering Corrections in $B_{u,d}\to VV$ Decays</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Fits of Weak Annihilation and Hard Spectator Scattering Corrections in $B_{u,d}\to VV$ Decays</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
