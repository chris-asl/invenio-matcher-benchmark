  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">1D topological chains with Majorana fermions in 2D non-topological optical lattices</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-03-05</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
