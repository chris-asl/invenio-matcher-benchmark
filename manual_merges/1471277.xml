  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Non-standard interactions and the resolution of ordering of neutrino masses at DUNE and other long baseline experiments</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Non-standard interactions and the resolution of ordering of neutrino masses at DUNE and other long baseline experiments</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2016-06-17</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
