  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Geometrical method for thermal instability of nonlinearly charged BTZ Black Holes</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Geometrical method for thermal instability of nonlinearly charged BTZ Black Holes</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-09-16</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
