  <datafield tag="242" ind1=" " ind2=" ">
    <subfield code="a">On the supermembrane with central charges and calibrated supergravity</subfield>
  </datafield>
  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Sobre la supermembrana con cargas centrales y la supergravedad calibrada</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
    <subfield code="a">211</subfield>
  </datafield>
  <datafield tag="500" ind1=" " ind2=" ">
