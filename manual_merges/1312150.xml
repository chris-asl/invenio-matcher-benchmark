  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Extended bodies in a Kerr spacetime: exploring the role of a general quadrupole tensor</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Extended bodies in a Kerr spacetime: exploring the role of a general quadrupole tensor</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-08-23</subfield>
  </datafield>
