  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Thermodynamic analysis of topological black holes in Gauss-Bonnet gravity with nonlinear source</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
