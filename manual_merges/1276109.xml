  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Extended Kerr-Schild spacetimes: General properties and some explicit examples</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Extended Kerr-Schild spacetimes: General properties and some explicit examples</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-01-06</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
