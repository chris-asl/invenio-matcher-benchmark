  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Tidal wave in $^{102}$Pd: An extended five-dimensional collective Hamiltonian description</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Tidal wave in $^{102}$Pd: An extended five-dimensional collective Hamiltonian description</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
