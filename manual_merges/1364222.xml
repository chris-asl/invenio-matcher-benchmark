  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Status of E-XFEL String and Cryomodule Assembly at CEA-Saclay</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2013-04</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
