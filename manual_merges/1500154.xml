  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Parity-Even and Time-Reversal-Odd Neutron Optical Potential in Spinning Matter Induced by Gravitational Torsion</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Parity-Even and Time-Reversal-Odd Neutron Optical Potential in Spinning Matter Induced by Gravitational Torsion</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
