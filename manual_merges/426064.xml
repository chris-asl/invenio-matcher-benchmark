  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On hard exclusive reactions in the timelike region</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">On hard exclusive reactions in the time-like region.</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
