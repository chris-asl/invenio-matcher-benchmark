  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Introducing SummerTime: a package for high-precision computation of sums appearing in DRA method</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Introducing SummerTime: a package for high-precision computation of sums appearing in DRA method</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-07-15</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
