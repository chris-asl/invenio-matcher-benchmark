  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Multi-Higgs doublet models with local $U(1)_H$ gauge symmetry and neutrino physics therein</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Multi-Higgs doublet models with local $U(1)_H$ gauge symmetry and neutrino physics therein</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
