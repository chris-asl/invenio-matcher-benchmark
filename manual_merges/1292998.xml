  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Reconstruction of $f(G)$ Gravity with New Agegraphic Dark Energy Model</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2014-04-28</subfield>
  </datafield>
