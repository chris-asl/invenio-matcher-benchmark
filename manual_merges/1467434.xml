  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Phase transition and thermodynamic stability in extended phase space and charged Ho\v{r}ava-Lifshitz black holes</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
