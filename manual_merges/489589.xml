  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Rotating nuclear rings and extreme starbursts in ultraluminous galaxies</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Rotating Nuclear Rings and Extreme Starbursts in Ultraluminous Galaxies</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
