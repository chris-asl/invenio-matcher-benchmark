  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Primordial magnetic fields in the $f^{2}FF$ model in large field inflation under de Sitter and power law expansion</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Primordial magnetic fields in the $f^{2}FF$ model in large field inflation under de Sitter and power law expansion</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2016-01-09</subfield>
  </datafield>
