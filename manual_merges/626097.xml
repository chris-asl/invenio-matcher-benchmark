  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Casimir energy densities for parallel plate on background of conformally flat brane world geometries and cosmological constant problem</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Casimir energy densities for parallel plate on background of conformally flat brane-world geometries and cosmological constant problem.</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
