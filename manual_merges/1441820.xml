  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Quasi-free scattering in the 6 Li(d, dd) 4 He reaction at low bombarding energies</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1973</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
