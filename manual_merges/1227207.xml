  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Testing lepton flavor universality in terms of data of BES III and charm-tau factory</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Testing lepton flavor universality in terms of data of BES III and charm-tau factory</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-04-04</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
