  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Reference Design of the Mu2e Detector Solenoid</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-09-24</subfield>
