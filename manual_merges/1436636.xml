  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The photoneutron cross sections of 28 Si and 24 Mg</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1970</subfield>
    <subfield code="t">published</subfield>
  </datafield>
