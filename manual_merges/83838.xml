  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The dynamics of the pi Delta system and the pi n resonances d15(1670) and s11(1535)</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1973</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
