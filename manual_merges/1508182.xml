  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Temperature dependence of shear viscosity of $SU(3)$--gluodynamics within lattice simulation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Temperature dependence of shear viscosity of $SU(3)$--gluodynamics within lattice simulation</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2017-01-09</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
