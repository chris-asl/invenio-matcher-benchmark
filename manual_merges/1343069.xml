  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The effects of delta mesons on the baryonic direct Urca processes in neutron star matter</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">The effects of delta mesons on the baryonic direct Urca processes in neutron star matter</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
