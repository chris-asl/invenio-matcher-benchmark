  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Analytical Bethe Ansatz for open spin chains with soliton non preserving boundary conditions</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
