  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">{\alpha}-accompanied cold ternary fission of $^{238-244}$Pu isotopes in equatorial and collinear configuration</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">{\alpha}-accompanied cold ternary fission of $^{238-244}$Pu isotopes in equatorial and collinear configuration</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-04-09</subfield>
  </datafield>
