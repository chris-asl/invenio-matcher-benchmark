  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Symmetry protected topological order in magnetization plateau states</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
