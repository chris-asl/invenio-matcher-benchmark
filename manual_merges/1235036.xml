  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Analysis of $B_s\to \phi\mu^+\mu^-$ decay within supersymmetry</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Analysis of $B_s\to \phi\mu^+\mu^-$ decay within supersymmetry</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-05-21</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
