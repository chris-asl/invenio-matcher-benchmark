  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A two-step method for retrieving the longitudinal profile of an electron bunch from its coherent radiation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">A two-step method for retrieving the longitudinal profile of an electron bunch from its coherent radiation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
