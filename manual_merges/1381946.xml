  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Analysis of the Heavy Pseudoscalar Mesons with Thermal QCD Sum Rules</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Analysis of the scalar nonet mesons with the QCD sum rules</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
