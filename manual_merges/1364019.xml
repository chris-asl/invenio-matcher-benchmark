  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A 10 MeV L-band Linac for Irradiation Applications in China</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2013-04</subfield>
  </datafield>
