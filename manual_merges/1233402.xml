  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Probe the 2$s_{1/2}$ and 1$d_{3/2}$ state level inversion with electron-nucleus scattering</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Probe the 2$s_{1/2}$ and 1$d_{3/2}$ state level inversion with electron-nucleus scattering</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
