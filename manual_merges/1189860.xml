  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Hadronic few-body systems in chiral dynamics, -- Few-body systems in hadron physics --</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
    <subfield code="a">8</subfield>
  </datafield>
  <datafield tag="500" ind1=" " ind2=" ">
