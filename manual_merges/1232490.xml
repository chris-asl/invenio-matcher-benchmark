  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Measurement of the expansion rate of the Universe from \gamma-ray attenuation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Measurement of the expansion rate of the Universe from \gamma-ray attenuation</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2013-05-09</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
