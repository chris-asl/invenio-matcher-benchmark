  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Photon emission and quantum signalling in biological systems</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-05-29</subfield>
