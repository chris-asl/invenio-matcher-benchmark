  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">PROTON HE-3 ELASTIC SCATTERING: A PHASE SHIFT ANALYSIS BY A SEPARABLE POTENTIAL MODEL</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1985</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
