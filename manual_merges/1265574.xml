  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The Pressure of QCD at Finite Temperature and Quark Number Density</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
    <subfield code="a">69</subfield>
  </datafield>
