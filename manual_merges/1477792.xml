  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Holographic bound in covariant loop quantum gravity</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
