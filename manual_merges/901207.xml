  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Implications of Yukawa texture in the charged Higgs boson phenomenology within 2HDM-III</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Implications of Yukawa texture in the charged Higgs boson phenomenology within 2HDM-III</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
