  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Empirical relation for total stopping power of positons and electrons</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1970</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
