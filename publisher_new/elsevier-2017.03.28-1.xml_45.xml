  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Hidden-charm molecular pentaquarks and their charm-strange partners</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-10</subfield>
