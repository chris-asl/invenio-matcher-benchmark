  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Algebro-geometric approach for a centrally extended U q [ sl (2|2)] R-matrix</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04</subfield>
