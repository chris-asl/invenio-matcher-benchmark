  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Duality and ( q -)multiple zeta values</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08-06</subfield>
