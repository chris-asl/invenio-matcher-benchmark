  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Synthesis of β-Ca 2 P 2 O 7 :Tb 3+ to gamma radiation detection by thermoluminescence</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-06</subfield>
