  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Performance of two mixed refrigerant processes providing refrigeration at 70 K</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09</subfield>
