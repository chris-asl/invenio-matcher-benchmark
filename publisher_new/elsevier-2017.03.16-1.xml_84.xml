  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Thermal diode made by nematic liquid crystal</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-07</subfield>
