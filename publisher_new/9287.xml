  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Nuclear stopping power evaluated via free protons emitted in reaction Xe + Sn near fermi energy</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-17</subfield>
