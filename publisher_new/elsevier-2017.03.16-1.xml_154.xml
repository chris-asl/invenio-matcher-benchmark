  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Second main theorem in the tropical projective space</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08-06</subfield>
    <subfield code="t">published</subfield>
