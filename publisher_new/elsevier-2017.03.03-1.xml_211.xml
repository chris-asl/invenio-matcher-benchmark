  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On the existence of stationary splash singularities for the Euler equations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-22</subfield>
    <subfield code="t">published</subfield>
