  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A type D structure in Khovanov homology</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-30</subfield>
