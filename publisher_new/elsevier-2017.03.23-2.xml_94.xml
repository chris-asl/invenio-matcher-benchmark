  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Integrable cluster dynamics of directed networks and pentagram maps</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-10</subfield>
    <subfield code="t">published</subfield>
