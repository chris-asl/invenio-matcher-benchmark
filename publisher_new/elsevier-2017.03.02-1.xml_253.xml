  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Probing Wilson loops in <math altimg="si1.gif" display="inline" overflow="scroll"><mi mathvariant="script">N</mi><mo>=</mo><mn>4</mn></math> Chern–Simons-matter theories at weak coupling</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-10</subfield>
    <subfield code="t">published</subfield>
  </datafield>
