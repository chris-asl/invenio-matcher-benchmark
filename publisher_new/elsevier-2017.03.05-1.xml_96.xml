  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Optimization of fast neutron flux in an irradiator assembly using Monte Carlo simulations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04</subfield>
    <subfield code="t">published</subfield>
