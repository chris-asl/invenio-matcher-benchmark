  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Detection and classification from electromagnetic induction data</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-15</subfield>
    <subfield code="t">published</subfield>
