  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Research and development of ion surfing RF carpets for the cyclotron gas stopper at the NSCL</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06-01</subfield>
