  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Magneto-oscillations on specific heat of graphene monolayer</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-28</subfield>
