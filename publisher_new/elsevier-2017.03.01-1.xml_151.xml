  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Merging analytic and empirical GEO debris synchronization dynamics</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-15</subfield>
