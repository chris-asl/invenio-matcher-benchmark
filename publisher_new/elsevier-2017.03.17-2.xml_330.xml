  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Pure spinors, intrinsic torsion and curvature in even dimensions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06</subfield>
