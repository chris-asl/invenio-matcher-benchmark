  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Electron scattering in a graphene nanoribbon in the presence of ferromagnetic layer and Rashba interaction</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-22</subfield>
    <subfield code="t">published</subfield>
