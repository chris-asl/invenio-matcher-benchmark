  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Titanium microgram weight low to 50 mg and measurement based on exchange weighing method</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12-27</subfield>
