  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A parameterization of the chemistry-normality dependence of bulk etch rate in a CR-39 detector</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12</subfield>
    <subfield code="t">published</subfield>
