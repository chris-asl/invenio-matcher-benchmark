  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Numerical study of the thermal behavior of bi-zone buildings</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-10</subfield>
