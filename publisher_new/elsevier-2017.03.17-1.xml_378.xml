  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Emergence of parochial altruism in well-mixed populations of multiple groups</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-10-09</subfield>
    <subfield code="t">published</subfield>
