  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Hirzebruch-type inequalities and plane curve configurations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-22</subfield>
