  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Complexity Induced Lifshitz Ordering with Multifractal Antiscreening/Screening (CILOMAS)</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-08</subfield>
    <subfield code="t">published</subfield>
