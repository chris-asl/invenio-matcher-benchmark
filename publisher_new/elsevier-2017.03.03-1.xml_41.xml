  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Current studies and future perspectives of synchrotron radiation imaging trials in human patients</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-11</subfield>
    <subfield code="t">published</subfield>
