  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On the fourth moment theorem for complex multiple Wiener–Itô integrals</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-08</subfield>
