  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Integrability of Hamiltonian systems with algebraic potentials</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-08</subfield>
