  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Determination of europium content in Li 2 SiO 3 (Eu) by neutron activation analysis using Am-Be neutron source</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
