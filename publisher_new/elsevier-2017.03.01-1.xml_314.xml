  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Electroweak bosons in Pb+Pb and p +Pb collisions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-01</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
