  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">2D Voronoi tessellation generated by lines and belts of dots</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-28</subfield>
