  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A hyperholomorphic line bundle on certain hyperkähler manifolds not admitting an S 1 -symmetry</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04</subfield>
    <subfield code="t">published</subfield>
