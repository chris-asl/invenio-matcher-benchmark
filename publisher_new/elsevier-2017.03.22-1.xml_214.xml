  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">CMB constraint on dark matter annihilation after Planck 2015</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-05-10</subfield>
