  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Non-linear duality invariant partially massless models?</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-10</subfield>
    <subfield code="t">published</subfield>
