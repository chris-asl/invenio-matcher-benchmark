  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Low voltage electrophoresis chip with multi-segments synchronized scanning</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-02</subfield>
