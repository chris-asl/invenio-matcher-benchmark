  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">NMR investigation of the quantum pigeonhole effect</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-05</subfield>
