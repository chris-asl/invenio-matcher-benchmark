  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Cubic modular equations in two variables</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-26</subfield>
