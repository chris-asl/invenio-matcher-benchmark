  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">1 K cryostat with sub-millikelvin stability based on a pulse-tube cryocooler</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01</subfield>
