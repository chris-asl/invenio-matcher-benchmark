  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Measurement and theoretical analysis of the excitation functions for 3 He induced reactions on nat Sn</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-01</subfield>
    <subfield code="t">published</subfield>
