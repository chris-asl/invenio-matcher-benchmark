  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A cut-off tubular geometry of loop space</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-01-31</subfield>
    <subfield code="t">published</subfield>
