  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Instrumentation challenges in multi-modality imaging</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-11</subfield>
