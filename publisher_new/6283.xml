  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Finite element investigation into the thermal conductivity of carbon nanotube/aluminum nanocomposites</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-22</subfield>
    <subfield code="t">published</subfield>
