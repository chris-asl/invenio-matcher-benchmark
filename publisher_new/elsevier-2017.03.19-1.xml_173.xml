  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Photofission cross-section ratio measurement of 235 U/ 238 U using monoenergetic photons in the energy range of 9.0–16.6 MeV</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-05-11</subfield>
    <subfield code="t">published</subfield>
