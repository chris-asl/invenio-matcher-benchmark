  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Trial equation method for solving the generalized Fisher equation with variable coefficients</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-22</subfield>
    <subfield code="t">published</subfield>
