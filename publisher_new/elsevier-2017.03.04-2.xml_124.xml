  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Simulation of photofission experiments at the ELI-NP facility</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-01</subfield>
