  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Effects of small-angle mistilts on dopant visibility in ADF-STEM imaging of nanocrystals</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-06</subfield>
    <subfield code="t">published</subfield>
