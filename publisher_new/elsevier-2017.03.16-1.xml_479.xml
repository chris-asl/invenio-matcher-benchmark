  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Analytical solutions in R + qR n cosmology from singularity analysis</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08-19</subfield>
