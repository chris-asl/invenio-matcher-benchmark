  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">High rate, fast timing Glass RPC for the high η CMS muon detectors</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-11</subfield>
