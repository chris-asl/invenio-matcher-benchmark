  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Poincaré series for tensor invariants and the McKay correspondence</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-26</subfield>
