  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Two-phase Cryogenic Avalanche Detector with electroluminescence gap operated in argon doped with nitrogen</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-11</subfield>
