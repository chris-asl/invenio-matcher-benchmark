  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Looking for quark saturation in proton and nuclei</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-15</subfield>
    <subfield code="t">published</subfield>
