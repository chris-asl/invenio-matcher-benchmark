  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Spot-welding solid targets for high current cyclotron irradiation</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12</subfield>
    <subfield code="t">published</subfield>
