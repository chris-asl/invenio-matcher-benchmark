  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Measurement of 1.7–74 MeV polarised γ rays with the HARPO TPC</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-11</subfield>
