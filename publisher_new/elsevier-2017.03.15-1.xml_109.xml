  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">All complete intersection varieties are Fano visitors</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04-30</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
