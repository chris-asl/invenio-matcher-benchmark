  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Insulating oxide surfaces and nanostructures</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-22</subfield>
