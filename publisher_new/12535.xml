  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On Einstein warped products with a quarter-symmetric connection</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-01-19</subfield>
    <subfield code="t">published</subfield>
