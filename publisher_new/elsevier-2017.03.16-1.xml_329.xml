  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Reciprocal link for a coupled Camassa–Holm type equation</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-08</subfield>
