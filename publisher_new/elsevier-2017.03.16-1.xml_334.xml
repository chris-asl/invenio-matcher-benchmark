  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The ϒ(1 S )→ B c ρ decay with perturbative QCD approach</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08</subfield>
