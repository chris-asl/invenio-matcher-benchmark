  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Sum rules and large deviations for spectral measures on the unit circle</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-03</subfield>
