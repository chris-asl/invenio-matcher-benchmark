  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Revalidation of the isobaric multiplet mass equation at A = 53, T = 3/2</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-05-10</subfield>
