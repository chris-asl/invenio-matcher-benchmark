  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A hybrid queuing strategy for network traffic on scale-free networks</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-15</subfield>
    <subfield code="t">published</subfield>
