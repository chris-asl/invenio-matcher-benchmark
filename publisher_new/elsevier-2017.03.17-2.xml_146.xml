  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Electromagnetic wave propagation in time-dependent media with antisymmetric magnetoelectric coupling</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
