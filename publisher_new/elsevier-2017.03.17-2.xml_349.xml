  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Inner structure of vehicular ensembles and random matrix theory</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-05-06</subfield>
    <subfield code="t">published</subfield>
