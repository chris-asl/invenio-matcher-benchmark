  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Experimental setup for radon exposure and first diffusion studies using gamma spectroscopy</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-01</subfield>
    <subfield code="t">published</subfield>
