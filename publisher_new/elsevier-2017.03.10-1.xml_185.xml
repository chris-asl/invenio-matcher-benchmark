  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Popularity and user diversity of online objects</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-01</subfield>
    <subfield code="t">published</subfield>
