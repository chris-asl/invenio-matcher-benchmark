  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Spherical vibrator model with an energy increasing stiffness</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-12-17</subfield>
