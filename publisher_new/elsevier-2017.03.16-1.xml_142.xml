  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Milnor–Witt K -groups of local rings</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-02</subfield>
