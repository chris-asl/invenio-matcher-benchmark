  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Quantum private comparison protocol without a third party</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-08</subfield>
    <subfield code="t">published</subfield>
