  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Exponential ergodicity for non-Lipschitz multivalued stochastic differential equations with Lévy jumps</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-03</subfield>
