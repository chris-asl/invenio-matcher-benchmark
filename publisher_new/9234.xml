  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A temperature characteristic research and compensation design for micro-machined gyroscope</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-22</subfield>
