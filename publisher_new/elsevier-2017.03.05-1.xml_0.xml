  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Oxidation processes in vanadium-based single-layer and nanolayer hard coatings</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04</subfield>
