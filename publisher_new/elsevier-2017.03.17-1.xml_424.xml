  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Collective behaviors of book holding durations</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-10-14</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
