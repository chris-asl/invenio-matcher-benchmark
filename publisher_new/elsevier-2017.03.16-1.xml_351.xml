  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Mori dream spaces and birational rigidity of Fano 3-folds</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-09</subfield>
