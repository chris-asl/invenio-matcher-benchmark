  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Radiation-hard ceramic Resistive Plate Chambers for forward TOF and T0 systems</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-11</subfield>
