  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">DEATH-STAR: Silicon and Photovoltaic Fission Fragment Detector Arrays for Light-Ion Induced Fission Correlation Studies</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-05-11</subfield>
