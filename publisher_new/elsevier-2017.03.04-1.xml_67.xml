  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Parameterization of ion channeling half-angles and minimum yields</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-15</subfield>
