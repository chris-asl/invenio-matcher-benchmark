  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Three dimensional characteristics of precipitating cloud systems observed during Indian summer monsoon</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-15</subfield>
