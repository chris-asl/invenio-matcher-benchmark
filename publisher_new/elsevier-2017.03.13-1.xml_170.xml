  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Differential cross sections measurement of 31 P(p,pγ 1 ) 31 P reaction for PIGE applications</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-15</subfield>
