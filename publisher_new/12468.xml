  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Numerical modeling of relativistic networks</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12-23</subfield>
