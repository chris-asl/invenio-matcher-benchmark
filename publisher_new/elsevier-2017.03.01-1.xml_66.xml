  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Theory of finite periodic systems: The eigenfunctions symmetries</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03</subfield>
