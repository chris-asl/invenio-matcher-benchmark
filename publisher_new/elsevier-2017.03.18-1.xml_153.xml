  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Fluctuating hydrodynamics for ionic liquids</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04-25</subfield>
    <subfield code="t">published</subfield>
