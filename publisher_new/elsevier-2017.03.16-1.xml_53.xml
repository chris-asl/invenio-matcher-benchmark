  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Dirac spectral flow on contact three manifolds II: Thurston–Winkelnkemper contact forms</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
