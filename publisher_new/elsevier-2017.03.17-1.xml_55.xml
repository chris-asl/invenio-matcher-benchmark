  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Twisted Ricci flow on a class of Finsler metrics and its solitons</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06</subfield>
    <subfield code="t">published</subfield>
