  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Low mass dimuon measurements in pp, p–Pb and Pb–Pb collisions with ALICE</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-01</subfield>
