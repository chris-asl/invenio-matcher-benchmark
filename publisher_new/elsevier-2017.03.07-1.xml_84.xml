  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Kinematics effects of atmospheric friction in spacecraft flybys</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04-01</subfield>
