  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">First Operational Phase of the Superconducting Magnet System of Wendelstein 7-X</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-16</subfield>
