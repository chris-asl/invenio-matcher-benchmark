  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Influence of the modulated hopping on the one-dimensional interacting electron system</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-10-09</subfield>
    <subfield code="t">published</subfield>
