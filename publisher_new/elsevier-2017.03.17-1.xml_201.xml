  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Innovation diffusion equations on correlated scale-free networks</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07-29</subfield>
