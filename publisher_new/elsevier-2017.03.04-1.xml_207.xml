  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Gradient estimates of mean curvature equations with Neumann boundary value problems</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-26</subfield>
