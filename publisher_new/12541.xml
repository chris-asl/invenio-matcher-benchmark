  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Spin jumping in the context of a QCD effective model</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-03</subfield>
    <subfield code="t">published</subfield>
