  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On the solution of elliptic partial differential equations on regions with corners</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-15</subfield>
    <subfield code="t">published</subfield>
