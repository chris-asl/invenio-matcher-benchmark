  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Combined SPECT/CT and PET/CT for breast imaging</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-11</subfield>
    <subfield code="t">published</subfield>
