  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Production cross sections of products in the proton induced reactions on nat Nd in the energy region up to 45 MeV</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-01</subfield>
