  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Top quark pair production measurements using the ATLAS detector at the LHC</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-04</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
