  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Performance of a multipurpose piezoelectric energy harvester</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-16</subfield>
