  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Effect of an in-plane magnetic field and a δ -doping on the electron transport in a nonmagnetic heterostructure</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-22</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
