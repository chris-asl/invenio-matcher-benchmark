  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Depth profiles of D and T in Metal-hydride films up to large depth</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-15</subfield>
