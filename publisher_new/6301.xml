  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Weyl static axially symmetric field equations in modified f(T) gravity</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-01-11</subfield>
    <subfield code="t">published</subfield>
