  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Center of the universal Askey–Wilson algebra at roots of unity</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-08</subfield>
