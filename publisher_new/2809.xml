  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Quantum AdaBoost algorithm via cluster state</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-30</subfield>
