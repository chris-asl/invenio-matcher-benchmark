  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Ionospheric characteristics prior to the greatest earthquake in recorded history</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-15</subfield>
