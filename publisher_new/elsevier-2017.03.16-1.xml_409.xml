  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Independent random partial matching with general types</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-02</subfield>
    <subfield code="t">published</subfield>
