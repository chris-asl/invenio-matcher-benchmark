  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Preclinical evaluation of 131 I-Bevacizumab – A prospective agent for radioimmunotherapy in VEGF expressing cancers</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-05</subfield>
    <subfield code="t">published</subfield>
