  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Cone-volume measure of general centered convex bodies</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-02</subfield>
