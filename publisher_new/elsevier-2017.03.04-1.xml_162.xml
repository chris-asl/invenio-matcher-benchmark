  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Cyclic Hodge integrals and loop Schur functions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-05</subfield>
