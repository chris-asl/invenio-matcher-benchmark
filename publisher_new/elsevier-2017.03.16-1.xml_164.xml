  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Liquid drop model of spherical nuclei with account of viscosity</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01</subfield>
