  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Laser processing in 3D diamond detectors</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-11</subfield>
