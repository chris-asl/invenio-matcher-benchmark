    <datafield ind1=" " ind2=" " tag="245">
      <subfield code="a">Two-loop amplitudes for qg→Hq and qq¯→Hg mediated by a nearly massless quark</subfield>
    </datafield>
    <datafield ind1=" " ind2=" " tag="260">
      <subfield code="c">2017-03-17</subfield>
