  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On weakly conformally symmetric pseudo-Riemannian manifolds</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-06</subfield>
    <subfield code="t">published</subfield>
