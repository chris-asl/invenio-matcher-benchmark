    <datafield ind1=" " ind2=" " tag="245">
      <subfield code="a">Measurement of the equation of state of solid-density copper heated with laser-accelerated protons</subfield>
    </datafield>
    <datafield ind1=" " ind2=" " tag="260">
      <subfield code="c">2017-03-13</subfield>
