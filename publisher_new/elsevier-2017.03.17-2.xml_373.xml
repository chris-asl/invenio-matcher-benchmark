  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Projectively flat Finsler metrics defined by the Euclidean metric and related 1-forms</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
