  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Tritium release from Li 4 SiO 4 ceramic pebbles in high magnetic field</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
