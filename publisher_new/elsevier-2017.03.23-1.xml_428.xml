  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Nash equilibrium and multi criterion aerodynamic optimization</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06-01</subfield>
