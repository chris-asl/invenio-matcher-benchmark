  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Vector fields with simply connected trajectories transverse to a polynomial</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-05</subfield>
