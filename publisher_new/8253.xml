 <datafield tag="245" ind1=" " ind2=" ">
  <subfield code="a">MagLev Cobra Project: Superconducting Levitation for Public Transportation</subfield>
  <subfield code="9">Sociedade Brasileira de Física</subfield>
 </datafield>
 <datafield tag="041" ind1=" " ind2=" ">
  <subfield code="a">Portuguese</subfield>
