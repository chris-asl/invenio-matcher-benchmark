  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Roles of clustering properties for degree-mixing pattern networks</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-17</subfield>
    <subfield code="t">published</subfield>
