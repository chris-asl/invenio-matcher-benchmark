  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Indoor 222 Rn concentration in the exhibition and storage rooms of Polish geological museums</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03</subfield>
