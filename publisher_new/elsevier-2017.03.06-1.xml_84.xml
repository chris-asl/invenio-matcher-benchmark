  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Concept design of DEMO divertor cassette remote handling: Simply supported beam approach</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03</subfield>
    <subfield code="t">published</subfield>
