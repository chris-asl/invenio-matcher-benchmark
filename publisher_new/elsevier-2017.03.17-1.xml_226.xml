  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The impact of static magnetic perturbations on edge localized modes</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07-29</subfield>
