  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Production and purification of Scandium-47: A potential radioisotope for cancer theranostics</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12</subfield>
