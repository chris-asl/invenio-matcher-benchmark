  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Study of ϒ( nS )→ B c P decays with perturbative QCD approach</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-12-17</subfield>
