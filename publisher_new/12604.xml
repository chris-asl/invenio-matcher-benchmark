  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Preparation of high-oriented molybdenum thin films using DC reactive magnetronsputtering</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-02</subfield>
