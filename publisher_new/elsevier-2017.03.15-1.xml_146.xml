  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Ion beam induced cubic to monoclinic phase transformation of nanocrystalline yttria</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07-15</subfield>
