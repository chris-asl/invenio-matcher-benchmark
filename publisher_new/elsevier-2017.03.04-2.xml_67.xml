  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Closed description of arbitrariness in resolving quantum master equation</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07-10</subfield>
