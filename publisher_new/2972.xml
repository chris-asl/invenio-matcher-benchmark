  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">All-transparent graphene-based flexible pressure sensor array</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-13</subfield>
