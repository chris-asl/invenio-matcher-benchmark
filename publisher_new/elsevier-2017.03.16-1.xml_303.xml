  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Duality, convexity and peak interpolation in the Drury–Arveson space</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06-04</subfield>
