  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">On quasisymmetry of quasiconformal mappings</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-22</subfield>
