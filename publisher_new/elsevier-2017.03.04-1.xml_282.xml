  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">In situ hydrogen isotope detection by ion beam methods ERDA and NRA</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-15</subfield>
