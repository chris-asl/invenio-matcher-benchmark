  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Large magnetocaloric effect in sintered ferromagnetic EuS</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-10</subfield>
    <subfield code="t">published</subfield>
