  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Cross section data for the D( 3 He,p) 4 He nuclear reaction from 0.25 to 6 MeV</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-03-15</subfield>
