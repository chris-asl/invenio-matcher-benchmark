  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Thermal stability of marks gold nanoparticles: A molecular dynamics simulation</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-17</subfield>
    <subfield code="t">published</subfield>
