  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Measurements of proton induced γ-ray emission cross-sections on Mg from 1.0 to 3.0 MeV</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04-01</subfield>
