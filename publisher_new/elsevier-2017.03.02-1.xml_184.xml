  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Learning and retrieval behavior in recurrent neural networks with pre-synaptic dependent homeostatic plasticity</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-02</subfield>
