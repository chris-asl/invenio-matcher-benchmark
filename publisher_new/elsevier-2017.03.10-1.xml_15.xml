  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Effective identification of multiple influential spreaders by DegreePunishment</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-01</subfield>
    <subfield code="t">published</subfield>
