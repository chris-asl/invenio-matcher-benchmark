  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Tropicalization of del Pezzo surfaces</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-10</subfield>
