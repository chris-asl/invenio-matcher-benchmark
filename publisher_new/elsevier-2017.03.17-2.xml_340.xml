  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">First-principles simulations of two dimensional electron gas near the interface of ZnO/GaN (0001) superlattice</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-10-09</subfield>
    <subfield code="t">published</subfield>
