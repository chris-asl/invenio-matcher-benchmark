  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Super-GCA from <math altimg="si1.gif" display="inline" overflow="scroll"><mi mathvariant="script">N</mi><mo>=</mo><mo stretchy="false">(</mo><mn>2</mn><mo>,</mo><mn>2</mn><mo stretchy="false">)</mo></math> super-Virasoro</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
