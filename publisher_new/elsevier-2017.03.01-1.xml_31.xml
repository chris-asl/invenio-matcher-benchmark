  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Active nutation control of an asymmetric spacecraft using an axial reaction wheel</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-12-01</subfield>
    <subfield code="t">published</subfield>
