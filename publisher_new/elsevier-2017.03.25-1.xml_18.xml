  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Trajectory refinement of three-body orbits in the real solar system model</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-04-15</subfield>
    <subfield code="t">published</subfield>
