  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Path-space variational inference for non-equilibrium coarse-grained systems</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-06-01</subfield>
