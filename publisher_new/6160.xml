  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Partial conservation law in a schematic single j shell model</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-01-13</subfield>
    <subfield code="t">published</subfield>
