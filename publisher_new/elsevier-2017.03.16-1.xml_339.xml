  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Suppression and revival of oscillation in indirectly coupled limit cycle oscillators</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-16</subfield>
    <subfield code="t">published</subfield>
