  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Radiation hard silicon particle detectors for HL-LHC—RD50 status report</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-02-11</subfield>
