  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Trading strategy based on dynamic mode decomposition: Tested in Chinese stock market</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-01</subfield>
    <subfield code="t">published</subfield>
