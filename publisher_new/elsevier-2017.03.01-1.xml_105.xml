  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Infrared radiometric measurements of lunar disk temperatures during lunar eclipse on 15th June 2011</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-09-15</subfield>
