  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Gaussian estimates on networks with dynamic stochastic boundary conditions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-03</subfield>
