  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Five-dimensional paracontact Lie algebras</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-04</subfield>
