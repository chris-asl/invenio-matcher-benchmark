  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Cross section versus time delay and trapping probability</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07-29</subfield>
    <subfield code="t">published</subfield>
