  <datafield tag="100" ind1=" " ind2=" ">
    <subfield code="a">Hogg, Anna E.</subfield>
    <subfield code="v">Centre for Polar Observation and Modelling, School of Earth and Environment, University of Leeds, Leeds, LS2 9JT, UK</subfield>
    <subfield code="m">A.E.Hogg@leeds.ac.uk</subfield>
