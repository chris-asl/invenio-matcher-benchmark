  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Thio residue from thermal processing of cometary ices containing carbon disulfide and ammonia</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
