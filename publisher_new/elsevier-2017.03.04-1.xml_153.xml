  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Stable soliton resolution for exterior wave maps in all equivariance classes</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2015-11-05</subfield>
