  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Establishing atmospheric neutrino oscillations with Super-Kamiokande</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-07</subfield>
