  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">MgB 2 wires with Ti and NbTi barrier made by IMD process</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-10</subfield>
    <subfield code="t">published</subfield>
