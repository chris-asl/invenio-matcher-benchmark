  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Measurement of hadron pair production cross sections at BESIII</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-03-04</subfield>
