  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Effects of energetic particles precipitation on stratospheric ozone in the Southern Hemisphere</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-11-15</subfield>
    <subfield code="t">published</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
