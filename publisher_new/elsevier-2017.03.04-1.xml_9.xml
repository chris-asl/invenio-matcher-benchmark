  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Congruence properties for a certain kind of partition functions</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-02-26</subfield>
